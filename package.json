        "url": "https://github.com/dobernike/my_finances/issues"
    },
    "homepage": "https://github.com/dobernike/my_finances#readme",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack --mode development",
        "build": "npm run build:legacy && npm run build:modern",
        "build:legacy": "cross-env NODE_ENV=production BROWSERSLIST_ENV=legacy webpack --mode production",
        "build:modern": "cross-env NODE_ENV=production BROWSERSLIST_ENV=modern webpack --mode production",
        "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
        "start": "cross-env NODE_ENV=development BROWSERSLIST_ENV=modern webpack-dev-server --mode development --open",
        "start:legacy": "cross-env NODE_ENV=development BROWSERSLIST_ENV=legacy webpack-dev-server --mode development --open",
        "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
        "test": "npm run jest:coverage && npm run lint",
        "jest": "jest --verbose --passWithNoTests",
        "jest:watch": "npm run jest --watchAll",
        "jest:coverage": "npm run jest --env=jsdom --coverage",
        "lint": "npm run lint:js && npm run prettier:css && npm run lint:css",
        "lint:js": "tsc --noEmit && eslint --fix src/**/*.{js,ts,tsx}",
        "lint:css": "stylelint --formatter=verbose src/**/*.css",
        "prettier:js": "prettier --write src/**/*.js",
        "prettier:css": "prettier-stylelint --write src/**/*.css"
    },
    "browserslist": {
        "legacy": [
            "> 1%",
            "ie 11"
        ],
        "modern": [
            "last 1 Chrome version",
            "last 1 Firefox version",
            "last 1 Safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run jest:coverage"
        }
    },
    "lint-staged": {
        "*.(js|ts|tsx)": [
            "npm run lint",
            "git add"
        ]
    },
